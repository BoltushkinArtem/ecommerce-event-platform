{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [ "FromLogContext", "WithThreadId", "WithMachineName" ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate":
          "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj} {Properties}{NewLine}{Exception}"
        }
      }
    ]
  },
  "Kafka": {
    "BootstrapServers": "",
    "Consumer": {
      "GroupId": "",
      "EnableAutoCommit": false
    },
    "Topics": [
      {
        "EventKey": "",
        "Name": ""
      }
    ],
    "Retry": {
      "RetryCount": 5,
      "BaseDelayMs": 100
    }
  }
}
